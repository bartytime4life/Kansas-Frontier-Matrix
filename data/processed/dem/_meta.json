  "gdal": {
    "add_gcps": "gdal_translate -of GTiff -a_srs EPSG:4326 $(printf \" %s\" $(awk -F'[,:{} ]+' '/px/{printf \"-gcp %s %s %s %s\", $3,$5,$9,$11}' data/gcp/LAWRENCE_1885.yml)) data/raw/ut_pcl/LAWRENCE_1885.jpg /tmp/LAWRENCE_1885_gcps.tif",
