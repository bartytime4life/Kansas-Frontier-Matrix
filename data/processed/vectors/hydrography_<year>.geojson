{
  "type": "FeatureCollection",
  "name": "hydrography_<year>",
  "crs": {
    "type": "name",
    "properties": { "name": "urn:ogc:def:crs:OGC:1.3:CRS84" }
  },
  "features": [],
  "properties": {
    "schema": {
      "id": "string",
      "name": "string",
      "feature_type": "string",      // stream | river | lake | reservoir | wetland | canal | spring | other
      "perennial": "boolean|null",   // true = perennial, false = intermittent/ephemeral, null = unknown
      "year_start": "integer|null",  // first known year (for multi-year spans)
      "year_end": "integer|null",    // last known year (inclusive); null if same as <year> or unknown
      "source": "string",            // e.g., 'USGS NHD', 'Historic Topo 1894 Larned', 'KDHE', 'DASC'
      "source_layer": "string|null", // e.g., service/layer name or sheet id
      "accuracy_m": "number|null",   // horiz positional accuracy estimate
      "notes": "string|null"         // free-form notes / OCR refs / map plate etc.
    }
  }
}
