  "stac_version":"1.0.0",
  "type":"Feature",
  "id":"dod-1993-2010",
  "collection":"bathymetry-hydrology",
  "geometry":{"type":"Polygon","coordinates":[...]},
  "bbox":[...],
  "properties":{
    "datetime":"2010-01-01T00:00:00Z",
    "kfm:parameter":"dod",
    "kfm:units":"meters_difference",
    "kfm:provider":"USACE",
    "kfm:method":"difference_of_dem",
    "kfm:site":"Tuttle_Creek_Forebay",
    "kfm:lineage":"etl/dod_1993_2010_v1",
    "kfm:quality":"A",
    "kfm:hydro_region":"Tuttle_Creek",
    "kfm:project":"Sedimentation-History"
  },
  "assets":{
